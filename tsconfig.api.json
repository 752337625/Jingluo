{
	"extends": "./tsconfig.web.json",
	"compilerOptions": {
		"allowJs": true,
		"noLib": false,
		"forceConsistentCasingInFileNames": true,
		"strictFunctionTypes": false,
		"noUnusedParameters": true,
		"useDefineForClassFields": true,
		"isolatedModules": true
	},
	"include": ["api/**/*.ts", "api/**/*.d.ts"]
}
