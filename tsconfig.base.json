{
	"compilerOptions": {
		"outDir": "dist",
		"target": "ESNext",
		"module": "ESNext",
		"rootDir": ".",
		"baseUrl": ".",
		"sourceMap": false,
		"moduleResolution": "Node",
		"allowJs": false,
		"strict": true,
		"noUnusedLocals": true,
		"resolveJsonModule": true,
		"allowSyntheticDefaultImports": true,
		"esModuleInterop": true,
		"removeComments": false,
		"types": [],
		"paths": {}
	}
}
